{
  "name": "returnsignals-www",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=24 <25"
  },
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview --host",
    "preview:test": "astro preview --host --port 4321",
    "lint": "eslint \"src/**/*.{js,ts,tsx,astro}\"",
    "lint:fix": "eslint \"src/**/*.{js,ts,tsx,astro}\" --fix",
    "typecheck": "astro check",
    "check": "npm run lint && npm run typecheck",
    "format": "prettier -w \"src/**/*.{ts,tsx,js,jsx,astro,css,md}\"",
    "generate-rewrites": "tsx scripts/generate-rewrites.ts"
  },
  "dependencies": {
    "@astrojs/sitemap": "^3.6.0",
    "@astrojs/tailwind": "^5.1.0",
    "@datadog/browser-logs": "^6.19.0",
    "@datadog/browser-rum": "^6.19.0",
    "@flaticon/flaticon-uicons": "^3.3.1",
    "astro": "^5.16.4"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.6",
    "@tailwindcss/typography": "^0.5.10",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^9.36.0",
    "eslint-plugin-astro": "^1.3.1",
    "prettier": "^3.2.5",
    "prettier-plugin-astro": "^0.14.1",
    "tailwindcss": "^3.4.0",
    "tsx": "^4.21.0",
    "typescript": "^5.4.0"
  },
  "packageManager": "npm@11.5.2"
}
